<%= form_for @ticket_task, url: new_ticket_task_path, html: { class: "ticket-task-form" } do |f|   %>

  <h5>Создание новой задачи</h5>

  <%= f.label :title, "Заголовок", class: "form-label" %>
  <%= f.text_field :title, class: "form-control" %>

  <%= f.label :description, "Описание", class: "form-label" %>
  <%= f.text_area :description, class: "form-control" %>

  <%= f.hidden_field :assigner_id, value: current_account.id %>
  <% f.label :assigner_id, "Назначающий", class: "form-label" %>

  <%= f.label :executor_id, "Исполнитель", class: "form-label" %>
  <%= f.collection_select :executor_id, Account.all, :id, :email, { prompt: "Выберите исполнителя" }, class: "form-control" %>

  <%= f.submit "Создать", class: "btn btn-primary" %>

<% end %>