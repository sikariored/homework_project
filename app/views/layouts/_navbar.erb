<% if account_signed_in? %>
  <div class="navbar">
    Профиль: <%= current_account.email %> | <%= current_account.role.role_name %>
    <%= button_to 'Выйти', destroy_account_session_path, method: :delete %>
    <%= link_to 'Заявки', tickets_path %> <br>
    <% if current_account.role_id == 6 %>
      <%= link_to 'Создать заявку', new_ticket_path %>
    <% end %>
  </div>
<% else %>
  <%= link_to 'Регистрация', new_account_registration_path %><br>
  <%= link_to 'Войти', account_session_path %><br>
<% end %>