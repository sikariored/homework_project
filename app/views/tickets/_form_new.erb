<%= form_for @ticket do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :body %>
  <%= f.text_area :body %>
  <br>
  <br>
  <%= f.hidden_field :account_id, value: current_account.id %>
  <%= f.hidden_field :status, value: 'Новая' %>

  <%= f.submit 'Create Ticket' %>
<% end %>