<%= form_for @ticket do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :body %>
  <%= f.text_area :body %>
  <br>
  <br>
  <%= f.hidden_field :account_id, value: current_account.id %>
  <%= f.hidden_field :ticket_status_id, value: TicketStatus.find_by(name: 'Новая')&.id %>

  <%= f.submit 'Create Ticket' %>
<% end %>